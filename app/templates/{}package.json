{
  "name": "<%=project%>",
  "version": "0.0.0",
  "description": "<%-description%>",
  >> if(license) {
  "license": "<%=license%>",
  >> }
  "repository": "https://github.com/<%=username%>/<%=project%>",
  "author": {
    >> if(authorName) {
    "name": "<%=authorName%>",
    >> }
    >> if(authorEmail) {
    "email": "<%=authorEmail%>",
    >> }
    >> if(authorUrl) {
    "url": "<%=authorUrl%>"
    >> }
  },
  "engines": {
    "node": ">=0.12.0"
  },
  >> if(babel) {
  "main": "dist",
  >> }
  "scripts": {
    >> if(babel) {
    "test": "mocha --compilers js:babel-register",
    >> } else {
    "test": "mocha"
    >> }
    >> if(babel && !gulp) {
    "build": "babel -d dist src",
    "postinstall": "npm run build",
    "watch": "nodemon -w src -w test -x \"npm run build && npm test\""
    >> }
  },
  >> if(cli) {
  "bin": {
    "<%=project%>": "bin.js"
  },
  "preferGlobal": true,
  >> }
  >> if(keywords.length) {
  "keywords": <%-keywordsFormatted%>,
  >> }
  "devDependencies": {
    "chai": "*",
    "mocha": "*"
  },
  "dependencies": <%-dependenciesFormatted%>
}
